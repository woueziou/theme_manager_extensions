{
    "manifest_version": 2,
    "name": "Theme manager",
    "version": "1.0",
    "description": "A simple extension that changes the background color of website",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }